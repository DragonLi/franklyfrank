data Nat = zero : Nat
         | suc : {[]Nat -> []Nat}.

data List X = nil : List | cons : {[]X -> []List -> []List}.

interface State X = get : X | put : {[]S -> []Unit}.

interface Receive X = receive : X.

fmap : {A -> B} -> List -> List
fmap f nil = nil;
fmap f (cons x xs) = cons (f! x) (fmap! f xs);

bind : X -> {X -> Y} -> Y
bind x f = f! x;

catter : [Receive (List X)](List X)
catter = bind! (receive!);
